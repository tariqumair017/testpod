  <script>

    var colors = [
      "--dark-green",
      "--light-brown",
      "--light-red",
      "--light-teal",
      "--light-gray"
    ];

   let window_Width=  window.innerWidth
    var swiperZoom = new Swiper(".swiper-container", {
      grabCursor: true,
      centeredSlides: true,
      
      slidesPerView: window_Width >= 700 ? "2" : "1",
      autoplay: {
        delay: 2000,
      },

      effect: "coverflow",
      keyboard: true,

      coverflowEffect: {
        rotate: 0,
        stretch: 0,
        depth: 100,
        modifier: 4,
        slideShadows: false
      }
    });

    swiperZoom.on("slideChange", function () {
      var index = this.activeIndex;
      document.querySelector(".single-view-content").style.background = "var(" + colors[index % colors.length] + ")";

      var teamInfoItems = document.querySelectorAll(".team-info .team-info-item");
      var activeIndex = this.activeIndex;

      teamInfoItems.forEach(function (item, index) {
        item.classList.remove("active");

        if (index === activeIndex) {
          item.classList.add("active");
        }
      });
    });
  </script>