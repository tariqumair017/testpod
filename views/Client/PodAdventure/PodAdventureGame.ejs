        <section class="pod_adventure_result_screen d-none">
            <div class="top-left-image">
                <img src="/client/img/padAdventure/TopLeft.svg" alt="" />
            </div>
            <div class="top-right-image">
                <img src="/client/img/padAdventure/TopRight.svg" alt="" />
            </div>
            <div class="bottom-left-image">
                <img width="85%" src="/client/img/padAdventure/falg.png" alt="" />
            </div>
            <section
                style="height: 55vh;display: flex;flex-direction: column;align-items: center;position: relative;gap: 5px;justify-content: center;">
                <div class="result-chr-png">
                    <img src="/client/img/png/png.svg" />
                </div>
                <div class="result-chr-png-shadow">
                    <img src="/client/img/png/chr-shadow.svg" />
                </div>

                <div style="display: flex;flex-direction: column;gap: 50px;">
                    <div>
                        <h2>Exploring Star!</h2>
                        <p style="margin: 0%;">
                            You have explored 4 countries of Asia
                        </p>
                    </div>
                    <div class="pod-result-box">
                        <img src="/client/img/padAdventure/icons8-goal-64.png" />
                        <p class="pod-percentage" style="margin: 0%;font-size:x-large;font-weight: 700;color: #000;">100%</p>
                    </div>
                </div>

            </section>
            <section class="pod-adventure-footer">
                <div class="pod-adventure-footer-inner controller">
                    <div style="width:100%;">
                        <div class="pod-adventure-game" style="justify-content: space-around;width: 100%;">
                            <div class="controller_buttons">
                                <a href="/pod-adtenture"><button class="pod-adventure-skip2"
