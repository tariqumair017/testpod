            </section>
            <section class="pod-adventure-footer">
                <div class="pod-adventure-footer-inner controller">
                    <div style="width: 100%;">
                        <div class="pod-adventure-game" style="justify-content: space-around;width: 100%;">
                            <div class="controller_buttons">
                                <button class="pod-adventure-skip2 skip-guess-flag" role="button">I Don't
                                    Know</button>
                                <div class="worng-area">
                                    <!-- <span class="cross-guess-flag d-none">
                                    <i class="fa-solid fa-xmark fa-2xl" style="color: #EA2B2B;"></i>
                                </span> -->
                                    <!-- <span class="right-guess-flag ">
                                    <i class="fa-solid fa-check fa-2xl" style="color: #61B800;"></i>
                                </span> -->
                                    <div class="well_done d-none">
                                        <img class="well_done_img" src="/client/img/icon/Vector-tick.svg" />
                                        <h4 style="margin-bottom: 0%;padding: 10px;">Well done!</h4>
                                    </div>
                                    <div class="Not_true d-none">
                                        <img class="Not_true_img" src="/client/img/icon/Vector.svg" />
                                        <h4 style="margin-bottom: 0%;padding: 10px;">Wrong</h4>
                                    </div>
                                </div>
                            </div>
                            <button class="pod-adventure-skip2-disabled check-guess-flag">Check</button>
                            <button class="well_done-continue continue-guess-flag d-none" role="button">
                                <!-- <img
                                src="/client/img/icon/VectorWrite.svg" /> -->
                                <h4 style="margin-bottom: 0%;padding: 10px;">Continue</h4>
                            </button>
                            <button class="well_done-continue game-change d-none" role="button">
