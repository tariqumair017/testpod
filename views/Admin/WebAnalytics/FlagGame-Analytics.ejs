<% layout('Admin/layouts/boilerPlate') -%>


<section style="padding: 20px;">

    <h3 >Analysis Flag Game</h3>
    

<table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Game</th>
        <th scope="col">views</th>
        <th scope="col">Completed</th>
        <th scope="col">Incomplete</th>
      </tr>
    </thead>
    <tbody>
      <% var count = 1%>
      <%data.forEach(item => {%>    
      <tr>
        <th scope="row"><%=count%></th>
        <td><%=item.gameName%></td>
        <td><%=item.logs?.views%></td>
        <td><%=(item.results.filter(x => x.status == 'complete')).length%></td>
        <td><%=(item.results.filter(x => x.status == 'Incomplete')).length%></td>
      </tr>
      <%count++%>
      <%})%>
    </tbody>
  </table>

</section>